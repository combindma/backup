{
    "name": "combindma/backup",
    "description": "Backup Laravel Package",
    "keywords": [
        "combindma",
        "laravel",
        "backup"
    ],
    "homepage": "https://github.com/combindma/backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "hello@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "combindma/dashui": "^1.0",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-backup": "^7.3",
        "spatie/laravel-package-tools": "^1.4.3",
        "ext-zip": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\Backup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\Backup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-filter": "vendor/bin/phpunit --no-coverage --filter",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\Backup\\BackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
